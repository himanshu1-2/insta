{
    "version": 2,
    "name": "insta_server",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
        },
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    ]
}
